# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# News API (Opcional - para notícias reais no modo stealth)
NEXT_PUBLIC_NEWS_API_KEY=your-news-api-key-here

# Push disfarçado de notícia - gerar com: node scripts/generate-vapid.js
NEXT_PUBLIC_VAPID_PUBLIC_KEY=your-vapid-public-key
VAPID_PRIVATE_KEY=your-vapid-private-key

# APNs (iOS push nativo) - chave .p8 do Apple Developer
APNS_KEY_P8="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----"
APNS_KEY_ID=your-key-id
APNS_TEAM_ID=your-team-id
APNS_BUNDLE_ID=com.noticias24h.app
APNS_PRODUCTION=true

# Node Environment
NODE_ENV=development
